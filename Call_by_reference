#include <stdio.h>

     void swap(int *x, int *y)
  	 {
  	   int z;
        z = *x;
 	   *x = *y;
 	   *y = z;

 	 }
 	 int main (int a, int b)
 	 {
 	   a = 7, b = 4;
 	   printf("Original values areL a = %d and b = %d\n", a, b);
 	   swap(&a, &b);
 	   printf("The values after swap are: a = %d and b = %d", a, b);

 	   return 0;
 	 }
