
#include<stdio.h>

int main(){
int a[100],n,i, sum=0;
float avg;
printf("Number of Inputs: ");
scanf("%d", &n);
printf("\Enter the element of find average: ");

for (i=0;i<n;i++){
scanf ("%d",&a[i]);
sum=sum+a[i];
}
printf ("sum=%d",sum);
avg=(float)sum/n;
printf ("\naverage is %f", avg);

return 0;
}

