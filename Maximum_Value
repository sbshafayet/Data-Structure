

#include<stdio.h>

int main()
{
int a[100], i, n, min;
printf("Number of Inputs: ");
scanf(" %d", &n);
printf("\Enter the element of find minimum: ");
for(i=0 ; i<n ; i++ )
scanf(" %d", &a[i]);
min = a[0];
for(i=0 ; i<n ; i++ )
if(a[i] < min)
min = a[i];
printf("minimum is %d ", min);

return 0;
}
